<!DOCTYPE html>
<html lang="en">

<head>
	{% include 'includes/meta.twig' %}
	<link href="{{ baseUrl }}/metronic/assets/vendors/base/vendors.bundle.css" rel="stylesheet" type="text/css" />
	<link href="{{ baseUrl }}/metronic/assets/demo/default/base/style.bundle.css" rel="stylesheet" type="text/css" />
	{% block css %}{% endblock %}
</head>

<body>
	{% include 'includes/metronic/header.twig' %}
	{% include 'includes/metronic/sidebar.twig' %}
	{% block content %}{% endblock %}
	{% block scripts %}{% endblock %}
	{% include 'includes/metronic/footer.twig' %}
	<script src="{{ baseUrl }}/js/bundle.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
        WebFont.load({
        google: {"families":["Poppins:300,400,500,600,700","Roboto:300,400,500,600,700"]},
        active: function() {
            sessionStorage.fonts = true;
        }
        });
    </script>
</body>
</html>
